{
  "version": 2,
  "builds": [
    {
      "src": "api/tiktok.js",
      "use": "@vercel/node"
    },
    {
      "src": "tiktok/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/tiktok",
      "dest": "/api/tiktok.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}